{
  "name": "simple-neo4j",
  "version": "4.3.0",
  "description": "Simple Neo4J database driver utilizing cypher query language",
  "main": "build/index.js",
  "scripts": {
    "compile": "babel -d ./build/ ./src/",
    "docs": "jsdoc -c ./.jsdocrc",
    "prepublish": "npm run eslint && npm run build && npm run test",
    "build": "npm run compile && npm run docs",
    "eslint": "node_modules/.bin/eslint -c .eslintrc ./src/",
    "pages": "git checkout gh-pages && git merge master && git checkout dev",
    "master": "git checkout master && git merge dev && git checkout dev",
    "test": "jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:RussellsToolbox/simple-neo4j.git"
  },
  "keywords": [
    "neo4j",
    "cypher"
  ],
  "author": "John Mothershed",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RussellsToolbox/simple-neo4j/issues"
  },
  "homepage": "https://github.com/RussellsToolbox/simple-neo4j#readme",
  "dependencies": {
    "babel-runtime": "^6.3.13",
    "cypher-tools": "^1.4.4",
    "neo4j-parser": "^2.0.4",
    "neo4j-query-object": "^2.0.5",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "eslint": "^1.10.3",
    "jasmine": "^2.4.1",
    "jsdoc": "^3.4.0",
    "node-inspector": "^0.12.5"
  },
  "tonicExampleFilename": "example.js"
}
